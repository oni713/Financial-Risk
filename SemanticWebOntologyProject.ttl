@prefix : <http://www.semanticweb.org/financial_risk#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fibo: <http://www.omg.org/spec/EDMC-FIBO/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.w3.org/2006/time#> .
@base <http://www.semanticweb.org/financial_risk#> .

<http://www.semanticweb.org/financial_risk> rdf:type owl:Ontology ;
                                             owl:imports fibo: ;
                                             rdfs:comment "A comprehensive ontology for financial risk management incorporating Bank of Albania, INSTAT, and international financial data" ;
                                             rdfs:label "Financial Risk Management Ontology" ;
                                             owl:versionInfo "1.0" .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/financial_risk#exposedToRisk
:exposedToRisk rdf:type owl:ObjectProperty ;
               rdfs:domain :FinancialEntity ;
               rdfs:range :RiskCategory ;
               rdfs:comment "Financial entity is exposed to specific risk" ;
               rdfs:label "exposed to risk" .


###  http://www.semanticweb.org/financial_risk#hasInsurancePolicy
:hasInsurancePolicy rdf:type owl:ObjectProperty ;
                    rdfs:domain :Person ;
                    rdfs:range :InsurancePolicy ;
                    rdfs:comment "Person has an insurance policy" ;
                    rdfs:label "has insurance policy" .


###  http://www.semanticweb.org/financial_risk#hasInvestment
:hasInvestment rdf:type owl:ObjectProperty ;
               rdfs:domain :Person ;
               rdfs:range :Investment ;
               rdfs:comment "Person has an investment" ;
               rdfs:label "has investment" .


###  http://www.semanticweb.org/financial_risk#hasLoan
:hasLoan rdf:type owl:ObjectProperty ;
         rdfs:domain :Person ;
         rdfs:range :Loan ;
         rdfs:comment "Person has a loan" ;
         rdfs:label "has loan" .


###  http://www.semanticweb.org/financial_risk#hasRetirementAccount
:hasRetirementAccount rdf:type owl:ObjectProperty ;
                      rdfs:domain :Person ;
                      rdfs:range :RetirementAccount ;
                      rdfs:comment "Person has a retirement account" ;
                      rdfs:label "has retirement account" .


###  http://www.semanticweb.org/financial_risk#hasRiskCategory
:hasRiskCategory rdf:type owl:ObjectProperty ;
                 rdfs:range :RiskCategory ;
                 rdfs:comment "Risk profile includes specific risk categories" ;
                 rdfs:label "has risk category" .


###  http://www.semanticweb.org/financial_risk#hasRiskProfile
:hasRiskProfile rdf:type owl:ObjectProperty ;
                rdfs:domain :Person ;
                rdfs:comment "Person has a specific risk profile" ;
                rdfs:label "has risk profile" .


###  http://www.semanticweb.org/financial_risk#influencedBy
:influencedBy rdf:type owl:ObjectProperty ,
                       owl:TransitiveProperty ;
              rdfs:domain :RiskCategory ;
              rdfs:range :EconomicIndicator ;
              rdfs:comment "Risk category is influenced by economic indicators" ;
              rdfs:label "influenced by" .


###  http://www.semanticweb.org/financial_risk#issuedBy
:issuedBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :issues ;
          rdfs:domain :Loan ;
          rdfs:range :Bank ;
          rdfs:comment "Loan is issued by a bank" ;
          rdfs:label "issued by" .


###  http://www.semanticweb.org/financial_risk#issues
:issues rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/financial_risk#managedBy
:managedBy rdf:type owl:ObjectProperty ;
           rdfs:domain :Investment ;
           rdfs:range :FinancialInstitution ;
           rdfs:comment "Investment is managed by financial institution" ;
           rdfs:label "managed by" .


###  http://www.semanticweb.org/financial_risk#operatesIn
:operatesIn rdf:type owl:ObjectProperty ;
            rdfs:domain :Bank ;
            rdfs:range :Country ;
            rdfs:comment "Bank operates in a country" ;
            rdfs:label "operates in" .


###  http://www.semanticweb.org/financial_risk#performsTransaction
:performsTransaction rdf:type owl:ObjectProperty ;
                     rdfs:domain :Person ;
                     rdfs:range :Transaction ;
                     rdfs:comment "Person performs a transaction" ;
                     rdfs:label "performs transaction" .


###  http://www.semanticweb.org/financial_risk#recommendsSimilar
:recommendsSimilar rdf:type owl:ObjectProperty ,
                            owl:SymmetricProperty ;
                   rdfs:domain :FinancialEntity ;
                   rdfs:range :FinancialEntity ;
                   rdfs:comment "Financial entity recommends similar entities" ;
                   rdfs:label "recommends similar" .


###  http://www.semanticweb.org/financial_risk#regulatedBy
:regulatedBy rdf:type owl:ObjectProperty ;
             owl:inverseOf :regulates ;
             rdfs:domain :SecondLevelBank ;
             rdfs:range :CentralBank ;
             rdfs:comment "Second level bank is regulated by central bank" ;
             rdfs:label "regulated by" .


###  http://www.semanticweb.org/financial_risk#regulates
:regulates rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/financial_risk#relatedToIndicator
:relatedToIndicator rdf:type owl:ObjectProperty ;
                    rdfs:range :EconomicIndicator ;
                    rdfs:comment "Risk profile is related to economic indicator" ;
                    rdfs:label "related to indicator" .


###  http://www.semanticweb.org/financial_risk#securingAsset
:securingAsset rdf:type owl:ObjectProperty ;
               rdfs:domain :Mortgage ;
               rdfs:range :RealEstateInvestment ;
               rdfs:comment "Mortgage is secured by real estate asset" ;
               rdfs:label "securing asset" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/financial_risk#hasAge
:hasAge rdf:type owl:DatatypeProperty ,
                 owl:FunctionalProperty ;
        rdfs:domain :Person ;
        rdfs:range xsd:integer ;
        rdfs:comment "Age of the person" ;
        rdfs:label "has age" .


###  http://www.semanticweb.org/financial_risk#hasAmount
:hasAmount rdf:type owl:DatatypeProperty ;
           rdfs:domain :FinancialEntity ;
           rdfs:range xsd:decimal ;
           rdfs:comment "Financial amount in EUR" ,
                        "Monetary amounts expressed in EUR" ;
           rdfs:label "has amount" .


###  http://www.semanticweb.org/financial_risk#hasAnnualReturn
:hasAnnualReturn rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Investment ;
                 rdfs:range xsd:decimal ;
                 rdfs:comment "Annual return percentage" ;
                 rdfs:label "has annual return" .


###  http://www.semanticweb.org/financial_risk#hasCreditRating
:hasCreditRating rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Person ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Credit rating classification" ;
                 rdfs:label "has credit rating" .


###  http://www.semanticweb.org/financial_risk#hasDateIssued
:hasDateIssued rdf:type owl:DatatypeProperty ;
               rdfs:domain :Loan ;
               rdfs:range xsd:dateTime ;
               rdfs:comment "Date when loan was issued" ;
               rdfs:label "has date issued" .


###  http://www.semanticweb.org/financial_risk#hasGDPValue
:hasGDPValue rdf:type owl:DatatypeProperty ;
             rdfs:domain :GDPIndicator ;
             rdfs:range xsd:decimal ;
             rdfs:comment "GDP value in billions EUR" ;
             rdfs:label "has GDP value" .


###  http://www.semanticweb.org/financial_risk#hasInflationRate
:hasInflationRate rdf:type owl:DatatypeProperty ;
                  rdfs:domain :InflationIndicator ;
                  rdfs:range xsd:decimal ;
                  rdfs:comment "Inflation rate as percentage" ;
                  rdfs:label "has inflation rate" .


###  http://www.semanticweb.org/financial_risk#hasInterestRate
:hasInterestRate rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Loan ;
                 rdfs:range xsd:decimal ;
                 rdfs:comment "Interest rate as percentage" ,
                              "Interest rate expressed as annual percentage" ;
                 rdfs:label "has interest rate" .


###  http://www.semanticweb.org/financial_risk#hasLoanStatus
:hasLoanStatus rdf:type owl:DatatypeProperty ;
               rdfs:domain :Loan ;
               rdfs:range xsd:string ;
               rdfs:comment "Current status of the loan" ;
               rdfs:label "has loan status" .


###  http://www.semanticweb.org/financial_risk#hasLoanTerm
:hasLoanTerm rdf:type owl:DatatypeProperty ;
             rdfs:domain :Loan ;
             rdfs:range xsd:integer ;
             rdfs:comment "Loan term in months" ;
             rdfs:label "has loan term" .


###  http://www.semanticweb.org/financial_risk#hasMaturityDate
:hasMaturityDate rdf:type owl:DatatypeProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :Bond
                                             :Loan
                                           )
                             ] ;
                 rdfs:range xsd:dateTime ;
                 rdfs:comment "Maturity date of financial instrument" ;
                 rdfs:label "has maturity date" .


###  http://www.semanticweb.org/financial_risk#hasName
:hasName rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain owl:Thing ;
         rdfs:range xsd:string ;
         rdfs:comment "Name of the entity" ;
         rdfs:label "has name" .


###  http://www.semanticweb.org/financial_risk#hasRiskScore
:hasRiskScore rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:range xsd:decimal ;
              rdfs:comment "Risk score from 0.0 to 10.0" ,
                           "Risk score ranges from 0.0 (lowest risk) to 10.0 (highest risk)" ;
              rdfs:label "has risk score" .


###  http://www.semanticweb.org/financial_risk#hasTransactionDate
:hasTransactionDate rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Transaction ;
                    rdfs:range xsd:dateTime ;
                    rdfs:comment "Date of the transaction" ;
                    rdfs:label "has transaction date" .


###  http://www.semanticweb.org/financial_risk#hasUnemploymentRate
:hasUnemploymentRate rdf:type owl:DatatypeProperty ;
                     rdfs:domain :UnemploymentIndicator ;
                     rdfs:range xsd:decimal ;
                     rdfs:comment "Unemployment rate as percentage" ;
                     rdfs:label "has unemployment rate" .


#################################################################
#    Classes
#################################################################

###  http://www.omg.org/spec/EDMC-FIBO/CreditRisk
fibo:CreditRisk rdf:type owl:Class ;
                owl:equivalentClass :CreditRisk .


###  http://www.omg.org/spec/EDMC-FIBO/InterestRateRisk
fibo:InterestRateRisk rdf:type owl:Class ;
                      owl:equivalentClass :InterestRateRisk .


###  http://www.semanticweb.org/financial_risk#Bank
:Bank rdf:type owl:Class ;
      rdfs:subClassOf :FinancialInstitution ;
      rdfs:comment "Commercial or central banks" ;
      rdfs:label "Bank" .


###  http://www.semanticweb.org/financial_risk#Bond
:Bond rdf:type owl:Class ;
      rdfs:subClassOf :Investment ;
      rdfs:comment "Debt securities" ;
      rdfs:label "Bond" .


###  http://www.semanticweb.org/financial_risk#Business
:Business rdf:type owl:Class ;
          rdfs:subClassOf :Customer .


###  http://www.semanticweb.org/financial_risk#BusinessLoan
:BusinessLoan rdf:type owl:Class ;
              rdfs:subClassOf :Loan ;
              rdfs:comment "Loan for business purposes" ;
              rdfs:label "Business Loan" .


###  http://www.semanticweb.org/financial_risk#CentralBank
:CentralBank rdf:type owl:Class ;
             rdfs:subClassOf :Bank ;
             rdfs:comment "National central banks like Bank of Albania" ;
             rdfs:label "Central Bank" .


###  http://www.semanticweb.org/financial_risk#Country
:Country rdf:type owl:Class ;
         rdfs:comment "National territory" ;
         rdfs:label "Country" .


###  http://www.semanticweb.org/financial_risk#CreditRisk
:CreditRisk rdf:type owl:Class ;
            rdfs:subClassOf :RiskCategory ;
            rdfs:comment "Credit risk assessment based on BASEL III guidelines" ,
                         "Mapped to FIBO Credit Risk" ,
                         "Risk of default on obligations" ;
            rdfs:label "Credit Risk" .


###  http://www.semanticweb.org/financial_risk#Customer
:Customer rdf:type owl:Class ;
          rdfs:label "Customers" .


###  http://www.semanticweb.org/financial_risk#Deposit
:Deposit rdf:type owl:Class ;
         rdfs:subClassOf :Transaction ;
         rdfs:comment "Deposit transaction" ;
         rdfs:label "Deposit" .


###  http://www.semanticweb.org/financial_risk#DiversifiedInvestor
:DiversifiedInvestor rdf:type owl:Class ;
                     rdfs:subClassOf :Person ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasInvestment ;
                                       owl:minCardinality "3"^^xsd:nonNegativeInteger
                                     ] .


###  http://www.semanticweb.org/financial_risk#EconomicIndicator
:EconomicIndicator rdf:type owl:Class ;
                   rdfs:comment "Macroeconomic indicators" ;
                   rdfs:label "Economic Indicator" .


###  http://www.semanticweb.org/financial_risk#FinancialEntity
:FinancialEntity rdf:type owl:Class ;
                 rdfs:comment "Abstract class representing any financial entity" ;
                 rdfs:label "Financial Entity" .


###  http://www.semanticweb.org/financial_risk#FinancialInstitution
:FinancialInstitution rdf:type owl:Class ;
                      owl:equivalentClass <https://spec.edmcouncil.org/fibo/ontology/FinancialServiceProvider> ;
                      rdfs:subClassOf :FinancialEntity ;
                      rdfs:comment "Banks, credit unions, and other financial institutions" ;
                      rdfs:label "Financial Institution" .


###  http://www.semanticweb.org/financial_risk#FinancialProduct
:FinancialProduct rdf:type owl:Class ;
                  rdfs:subClassOf :FinancialEntity .


###  http://www.semanticweb.org/financial_risk#GDPIndicator
:GDPIndicator rdf:type owl:Class ;
              rdfs:subClassOf :EconomicIndicator ;
              rdfs:comment "Gross Domestic Product indicator" ;
              rdfs:label "GDP Indicator" .


###  http://www.semanticweb.org/financial_risk#GeneralRisk
:GeneralRisk rdf:type owl:Class ;
             rdfs:subClassOf :RiskCategory ;
             rdfs:comment "General financial risk" ;
             rdfs:label "General Risk" .


###  http://www.semanticweb.org/financial_risk#GovernmentBond
:GovernmentBond rdf:type owl:Class ;
                rdfs:subClassOf :Bond ;
                rdfs:comment "Bonds issued by government entities" ;
                rdfs:label "Government Bond" .


###  http://www.semanticweb.org/financial_risk#HighInterestLoan
:HighInterestLoan rdf:type owl:Class ;
                  rdfs:subClassOf :Loan ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasInterestRate ;
                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                         owl:onDatatype xsd:decimal ;
                                                         owl:withRestrictions ( [ xsd:minInclusive 8.0
                                                                                ]
                                                                              )
                                                       ]
                                  ] .


###  http://www.semanticweb.org/financial_risk#InflationIndicator
:InflationIndicator rdf:type owl:Class ;
                    rdfs:subClassOf :EconomicIndicator ;
                    rdfs:comment "Inflation rate indicator" ;
                    rdfs:label "Inflation Indicator" .


###  http://www.semanticweb.org/financial_risk#InsurancePolicy
:InsurancePolicy rdf:type owl:Class ;
                 rdfs:subClassOf :FinancialProduct ;
                 rdfs:comment "Insurance coverage policy" ;
                 rdfs:label "Insurance Policy" .


###  http://www.semanticweb.org/financial_risk#InterestRateRisk
:InterestRateRisk rdf:type owl:Class ;
                  rdfs:subClassOf :RiskCategory ;
                  rdfs:comment "Mapped to FIBO Interest Rate Risk" ,
                               "Risk from interest rate changes" ;
                  rdfs:label "Interest Rate Risk" .


###  http://www.semanticweb.org/financial_risk#Investment
:Investment rdf:type owl:Class ;
            rdfs:subClassOf :FinancialProduct ;
            rdfs:comment "Financial investment instrument" ;
            rdfs:label "Investment" .


###  http://www.semanticweb.org/financial_risk#LiquidityRisk
:LiquidityRisk rdf:type owl:Class ;
               rdfs:subClassOf :RiskCategory ;
               rdfs:comment "Risk of inability to meet short-term obligations" ;
               rdfs:label "Liquidity Risk" .


###  http://www.semanticweb.org/financial_risk#Loan
:Loan rdf:type owl:Class ;
      owl:equivalentClass <https://spec.edmcouncil.org/fibo/ontology/Loan> ;
      rdfs:subClassOf :FinancialProduct ;
      rdfs:comment "Financial loan with terms and conditions" ;
      rdfs:label "Loan" .


###  http://www.semanticweb.org/financial_risk#LowInterestLoan
:LowInterestLoan rdf:type owl:Class ;
                 rdfs:subClassOf :Loan .


###  http://www.semanticweb.org/financial_risk#MarketRisk
:MarketRisk rdf:type owl:Class ;
            rdfs:subClassOf :RiskCategory ;
            rdfs:comment "Market risk following EU financial regulations" ,
                         "Risk from market volatility" ;
            rdfs:label "Market Risk" .


###  http://www.semanticweb.org/financial_risk#Mortgage
:Mortgage rdf:type owl:Class ;
          rdfs:subClassOf :Loan ;
          rdfs:comment "Real estate secured loan" ;
          rdfs:label "Mortgage" .


###  http://www.semanticweb.org/financial_risk#MutualFund
:MutualFund rdf:type owl:Class ;
            rdfs:subClassOf :Investment ;
            rdfs:comment "Pooled investment vehicle" ;
            rdfs:label "Mutual Fund" .


###  http://www.semanticweb.org/financial_risk#Payment
:Payment rdf:type owl:Class ;
         rdfs:subClassOf :Transaction ;
         rdfs:comment "Outgoing payment transaction" ;
         rdfs:label "Payment" .


###  http://www.semanticweb.org/financial_risk#Pension
:Pension rdf:type owl:Class ;
         rdfs:subClassOf :RetirementAccount ;
         rdfs:comment "Employer-sponsored retirement plan" ;
         rdfs:label "Pension" .


###  http://www.semanticweb.org/financial_risk#Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf :Customer ;
        rdfs:comment "Individual person with financial profiles" ;
        rdfs:label "Person" .


###  http://www.semanticweb.org/financial_risk#PersonWithMultipleLoans
:PersonWithMultipleLoans rdf:type owl:Class ;
                         rdfs:subClassOf :Person ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasLoan ;
                                           owl:minCardinality "2"^^xsd:nonNegativeInteger
                                         ] .


###  http://www.semanticweb.org/financial_risk#PersonalLoan
:PersonalLoan rdf:type owl:Class ;
              rdfs:subClassOf :Loan ;
              rdfs:comment "Unsecured personal loan" ;
              rdfs:label "Personal Loan" .


###  http://www.semanticweb.org/financial_risk#RealEstateInvestment
:RealEstateInvestment rdf:type owl:Class ;
                      rdfs:subClassOf :Investment ;
                      rdfs:comment "Property investment" ;
                      rdfs:label "Real Estate Investment" .


###  http://www.semanticweb.org/financial_risk#RetirementAccount
:RetirementAccount rdf:type owl:Class ;
                   rdfs:subClassOf :FinancialProduct ;
                   rdfs:comment "Retirement savings account" ;
                   rdfs:label "Retirement Account" .


###  http://www.semanticweb.org/financial_risk#RiskCategory
:RiskCategory rdf:type owl:Class ;
              rdfs:comment "Classification of financial risks" ;
              rdfs:label "Risk Category" .


###  http://www.semanticweb.org/financial_risk#SecondLevelBank
:SecondLevelBank rdf:type owl:Class ;
                 rdfs:subClassOf :Bank ;
                 rdfs:comment "Commercial banks operating under central bank regulation" ;
                 rdfs:label "Second Level Bank" .


###  http://www.semanticweb.org/financial_risk#Stock
:Stock rdf:type owl:Class ;
       rdfs:subClassOf :Investment ;
       rdfs:comment "Equity shares in companies" ;
       rdfs:label "Stock" .


###  http://www.semanticweb.org/financial_risk#Transaction
:Transaction rdf:type owl:Class ;
             rdfs:subClassOf :FinancialEntity ;
             rdfs:comment "Financial transaction" ;
             rdfs:label "Transaction" .


###  http://www.semanticweb.org/financial_risk#UnemploymentIndicator
:UnemploymentIndicator rdf:type owl:Class ;
                       rdfs:subClassOf :EconomicIndicator ;
                       rdfs:comment "Unemployment rate indicator" ;
                       rdfs:label "Unemployment Indicator" .


###  http://www.semanticweb.org/financial_risk#Withdrawal
:Withdrawal rdf:type owl:Class ;
            rdfs:subClassOf :Transaction ;
            rdfs:comment "Withdrawal transaction" ;
            rdfs:label "Withdrawal" .


###  https://spec.edmcouncil.org/fibo/ontology/FinancialServiceProvider
<https://spec.edmcouncil.org/fibo/ontology/FinancialServiceProvider> rdf:type owl:Class ;
                                                                     rdfs:subClassOf :FinancialEntity .


###  https://spec.edmcouncil.org/fibo/ontology/Loan
<https://spec.edmcouncil.org/fibo/ontology/Loan> rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/financial_risk#Albania
:Albania rdf:type owl:NamedIndividual ,
                  :Country ;
         :hasName "Albania" .


###  http://www.semanticweb.org/financial_risk#AlbaniaGDP2023
:AlbaniaGDP2023 rdf:type owl:NamedIndividual ,
                         :GDPIndicator ;
                :hasGDPValue 17.8 ;
                :hasName "Albania GDP 2023" .


